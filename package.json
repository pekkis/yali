{
  "name": "@dr-kobros/react-broilerplate-project",
  "version": "1.0.0",
  "description": "An example project utilizing broilerplate",
  "main": "src/client.jsx",
  "scripts": {
    "clean": "rimraf ./dist",
    "build:webpack": "NODE_ENV=prod webpack",
    "build": "npm run clean && npm run build:webpack",
    "lint": "eslint --ext js --ext jsx .",
    "babel-node": "babel-node",
    "start": "NODE_ENV=development nodemon --exec npm run babel-node -w src/server.js -- src/server.js",
    "test": "ava -v"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pekkis/react-broilerplate-project"
  },
  "keywords": [
    "team"
  ],
  "author": "Pekkis",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@dr-kobros/react-broilerplate": "^0.7.0",
    "@dr-kobros/react-broilerplate-server-express": "^0.2.0",
    "axios": "^0.9.1",
    "body-parser": "^1.15.0",
    "classnames": "^2.2.3",
    "immutable": "^3.7.6",
    "node-uuid": "^1.4.7",
    "normalize.css": "^3.0.3",
    "random-js": "^1.0.8",
    "react-dnd": "^2.1.3",
    "react-dnd-html5-backend": "^2.1.2",
    "react-fa": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.3"
  }
}
